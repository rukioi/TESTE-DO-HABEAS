    ]
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "dev:backend": "tsx watch src/server.ts",
    "build:backend": "tsc -p tsconfig.server.json",
    "start:backend": "node ./dist/server.js",
    "build:full": "prisma generate && npm run build:backend && npm run build",
    "build:client": "prisma generate && npm run build:backend && npm run build",
    "start": "npm run start:backend",
    "db:init": "node scripts/init-database.js",
    "db:test": "node scripts/test-connection.js",
    "postinstall": "prisma generate"
